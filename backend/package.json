{
    "name": "bluegreen-backend",
    "version": "1.0.0",
    "description": "Backend simples para testes de Blue Green Deployment",
    "main": "server.js",
    "scripts": {
        "dev": "node --env-file=.env.development --watch server.js",
        "blue": "node --env-file=.env.production --watch server.js",
        "green": "node --env-file=.env.production --watch server.js",
        "pm2-dev": "pm2 start ecosystem.config.js --only dev",
        "pm2-blue": "pm2 start ecosystem.config.js --only blue",
        "pm2-green": "pm2 start ecosystem.config.js --only green"
    },
    "keywords": [
        "blue-green",
        "deployment",
        "test"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.18.2"
    }
}
